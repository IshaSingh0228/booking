pipeline{
   agent any;
   tools{
       maven 'maven3'
       jdk 'jdk11'
   }
  stages {
          stage("build & SonarQube analysis") {
            agent any
            steps {
              withSonarQubeEnv('sonar-server') {
                bat 'java -version'
                bat 'mvn clean package sonar:sonar'
              }
            }
          }
    
    stage("Uploading to artifactory"){
    steps{
    rtUpload (
    serverId: 'ARTIFACTORY_SERVER',
    spec: '''{
          "files": [
            {
              "pattern": "target/*.jar",
              "target": "art-doc-dev-loc"
            }
         ]
    }''',
)
}}
}
}
